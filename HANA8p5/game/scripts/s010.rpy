label s010:

    scene bg19a
    with fade

    play music "bgm/M13.ogg" fadein 1.0

    show amane tam03sa at c
    with dissolve

    window show
    voice "AMANE_0277"
    amane "「呼诶～，作业做完了～」"

    show kaori tka01sa at l
    show amane tam03sa at r
    with dissolve

    voice "KAORI_0257"
    kaori "「总算是赶上了呢。」"

    voice "AMANE_0278"
    amane "「好累啊～，头晕乎乎的～」"

    show kaori tka03sa at l

    voice "KAORI_0258"
    kaori "「这次我也有错。」"

    voice "AMANE_0279"
    amane "「诶？」"

    voice "KAORI_0259"
    kaori "「如果我有好好，督促天音把作业做完就好了。」"

    voice "AMANE_0280"
    amane "「啊……」"

    show kaori tka01sa at l

    voice "KAORI_0260"
    kaori "「天音的话，不可能不忘记作业嘛。」"

    show amane tam02sa at r

    voice "AMANE_0281"
    amane "「不愧是佳织酱，最了解我呢。」"

    voice "KAORI_0261"
    kaori "「因为我和你相处多少年了。」"

    voice "AMANE_0282"
    amane "「那个——，１，２，３……已经有１０年了！」"

    voice "KAORI_0262"
    kaori "「我们还是邻居，所以也不只１０年了吧。」"

    show amane tam01sa at r

    voice "AMANE_0283"
    amane "「啊，也对。我还以为是进了米卡女之后开始呢。」"

    voice "KAORI_0263"
    kaori "「……自从进入米卡女校后，已经过了这么久了啊。」"

    voice "AMANE_0284"
    amane "「没错哦。」"

    show kaori tka03sa at l

    voice "KAORI_0264"
    kaori "「既然这样不更加注重学业可不行。天音忘记做作业倒是家常便饭了。」"

    show amane tam03sa at r

    voice "AMANE_0285"
    amane "「好像我每次都会忘记写作业。」"

    show kaori tka08sa at l

    voice "KAORI_0265"
    kaori "「我不说的话，不是几乎每次都不记得么。」"

    voice "AMANE_0286"
    amane "「佳织酱这么说起来，好像就是这样。」"

    show kaori tka03sa at l

    voice "KAORI_0266"
    kaori "「哈——，天音变得这么依赖别人，或许是我的错呢。」"

    show amane tam02sa at r

    voice "AMANE_0287"
    amane "「可是，多亏那样我也得救了哦。」"

    voice "KAORI_0267"
    kaori "「虽然或许是这样，你就没有自觉完成作业的意思吗？」"

    show amane tam03sa at r

    voice "AMANE_0288"
    amane "「大概有吧～，或许没有吧～，又像有吧～，可是果然还是没有吧～」"

    show kaori tka08sa at l

    voice "KAORI_0268"
    kaori "「是哪个啊？」"

    show amane tam01sa at r

    voice "AMANE_0289"
    amane "「不是啦，真的有，干劲。」"

    show amane tam02sa at r

    voice "AMANE_0290"
    amane "「因为，我呢，最喜欢看到佳织酱的作业了嘛。」"

    show kaori tka03sa at l

    voice "KAORI_0269"
    kaori "「哈啊？什么意思？」"

    voice "AMANE_0291"
    amane "「那个呢，就是说？像这样，喜欢和佳织酱两人在一起呢。」"

    show kaori tka04sa at l

    voice "KAORI_0270"
    kaori "「笨，笨蛋！在说什么奇怪的话！？」"

    show amane tam03sa at r

    voice "AMANE_0292"
    amane "「诶——，因为是真的嘛。」"

    show kaori tka05sa at l

    voice "KAORI_0271"
    kaori "「就，就算这样也不要现在……」"

    show amane tam02sa at r

    voice "AMANE_0293"
    amane "「呵呵呵～要永远在一起哦，佳织酱。」"

    show kaori tka04sa at l

    voice "KAORI_0272"
    kaori "「什……！？」"

    voice "AMANE_0294"
    amane "「就算以后变成大人了，变成老奶奶了，永远永远都要在一起哦。」"

    voice "KAORI_0273"
    kaori "「这，这样不就像求婚一样吗！」"

    show amane tam03sa at r

    voice "AMANE_0295"
    amane "「啊咧？是吗？」"

    show kaori tka07sa at l

    voice "KAORI_0274"
    kaori "「真是的！不要再说些奇怪的话给我集中精神完成作业啊。」"

    show amane tam06sa at r

    voice "AMANE_0296"
    amane "「呜哇啊啊？对不起——」"

    show kaori tka03sa at l

    voice "KAORI_0275"
    kaori "「哈啊哈啊……」"

    show amane tam09sa at r

    voice "AMANE_0297"
    amane "「呜，我说的可是认真的啊——」"

    voice "KAORI_0276"
    kaori "「哈啊哈啊……诶？你说什么？」"

    show amane tam01sa at r

    voice "AMANE_0298"
    amane "「不是，什么也没有——。」"

    show amane tam02sa at r

    voice "AMANE_0299"
    amane "「可是，这次真的得救了。谢谢你。」"

    voice "AMANE_0300"
    amane "「果然，最后可以帮助我的，还是佳织酱呢。」"

    show kaori tka05sa at l

    voice "KAORI_0277"
    kaori "「没什么……」"

    voice "AMANE_0301"
    amane "「呵呵，最喜欢佳织酱了！」"

    show kaori tka04sa at l

    voice "KAORI_0278"
    kaori "「！？」"

    voice "KAORI_0279"
    kaori "「你，你你，你呀，干嘛突然说这种奇怪的话！」"

    show amane tam03sa at r

    voice "AMANE_0302"
    amane "「诶？」"

    show kaori tka05sa at l

    voice "KAORI_0280"
    kaori "「在教室里说出这么害羞的话，你不觉得害羞吗？我都已经害羞得，说了这么多次害羞了啊！」"

    voice "AMANE_0303"
    amane "「哈诶～……？」"

    show kaori tka03sa at l

    voice "KAORI_0281"
    kaori "「咕，没有自觉……」"

    voice "AMANE_0304"
    amane "「咋了？佳织酱？」"

    show kaori tka07sa at l

    voice "KAORI_0282"
    kaori "「什么事都没有。」"

    show amane tam06sa at r

    voice "AMANE_0305"
    amane "「啊呜，生气啦……」"

    show kaori tka07sa at sl
    show koharu tko01sa at sr
    show amane tam06sa at c
    with dissolve

    voice "KOHARU_0081"
    koharu "「我想佳织只是腼腆而已哦。」"

    show kaori tka04sa at sl

    voice "KAORI_0283"
    kaori "「才，才不是，因为天音没有自觉——」"

    voice "KOHARU_0082"
    koharu "「嘛嘛。我也是从来到米卡女开始，就一直和你们两人在一起，所以明白。」"

    show kaori tka03sa at sl

    voice "KAORI_0284"
    kaori "「呜咕……小春……」"

    hide amane
    show makoto tma03sa at sr
    show koharu tko01sa at c
    with dissolve

    voice "MAKOTO_0046"
    makoto "「一直在一起吗……有些羡慕呢。」"

    show koharu tko02sa at c

    voice "KOHARU_0083"
    koharu "「真琴，从今以后也会制作出很多回忆吧。」"

    show makoto tma02sa at sr

    voice "MAKOTO_0047"
    makoto "「嗯。」"

    hide kaori
    hide makoto
    hide koharu
    show amane tam08sa at c
    with dissolve

    voice "AMANE_0306"
    amane "「我也会留下更多的回忆。」"

    voice "AMANE_0307"
    amane "「不只是和小真，还有佳织酱和小春也是呢。」"

    show kaori tka03sa at l
    show amane tam08sa at r
    with dissolve

    voice "KAORI_0285"
    kaori "「你说以后……制作出更多回忆吗……？」"

    show amane tam02sa at r

    voice "AMANE_0308"
    amane "「嗯，因为最喜欢了！」"

    show kaori tka05sa at l

    voice "KAORI_0286"
    kaori "「都，都说了，不要再说这种让人害羞的话了。」"

    show amane tam03sa at r

    voice "AMANE_0309"
    amane "「为什么？为什么会害羞呢？」"

    show amane tam02sa at r

    voice "AMANE_0310"
    amane "「对喜欢的人说喜欢是很普通的哦。」"

    show kaori tka04sa at l

    voice "KAORI_0287"
    kaori "「呀——！所以不要再说这种话啦——！」"

    hide kaori
    show amane tam03sa at r
    with dissolve

    voice "AMANE_0311"
    amane "「啊啊，佳织酱为什么要逃跑——！」"

    hide amane
    show kaori tka07sa at c
    with dissolve

    voice "KAORI_0288"
    kaori "「少，少啰嗦——！」"

    voice "KAORI_0289"
    kaori "「我想一个人静静！」"

    hide kaori
    show amane tam03sa at c
    with dissolve

    voice "AMANE_0312"
    amane "「啊嗯，等一下啦佳织酱——」"

    hide amane
    show kaori tka07sa at c
    with dissolve

    voice "KAORI_0290"
    kaori "「别跟过来——！」"

    hide kaori
    with dissolve

    play sound "se/SE012.ogg"

    show koharu tko01sa at c
    with dissolve

    voice "KOHARU_0084"
    koharu "「啊啊……跑掉了。」"

    show koharu tko01sa at l
    show makoto tma02sa at r
    with dissolve

    voice "MAKOTO_0048"
    makoto "「可以说出这些话，真是非常要好呢。」"

    show koharu tko02sa at l

    voice "KOHARU_0085"
    koharu "「呵呵呵……嗯，就是呢。」"

    stop voice
    window hide

    stop music fadeout 2.0
    scene black with fade2
    pause 1
    
    $ _rollback = False
    $ old_game_menu_screen = _game_menu_screen
    $ _game_menu_screen = None
    show eyecatch01 at eyecatch_in 
    $ renpy.pause(delay=2,hard=True)
    show eyecatch01 at eyecatch_out 
    $ renpy.pause(delay=5,hard=True)
    $ _game_menu_screen = old_game_menu_screen
    $ _rollback = True
    pause 1
    
    jump s011
