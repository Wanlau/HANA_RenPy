label s017:

    scene bg19a
    with gs_cud

    play music "bgm/M18.ogg" fadein 1.0

    show amane tam02sa at c
    with dissolve

    window show
    voice "AMANE_0457"
    amane "「哈嗯哈嗯，好吃——。下巴都要掉了。」"

    show kaori tka02sa at l
    show amane tam02sa at r
    with dissolve

    voice "KAORI_0423"
    kaori "「嗯。果然点心是装在另一个胃的呢——特别是小春的点心。」"

    show kaori tka02sa at sl
    show makoto tma02sa at sr
    show amane tam02sa at c
    with dissolve

    voice "MAKOTO_0127"
    makoto "「不愧是小春。」"

    hide kaori
    hide makoto
    hide amane
    show koharu tko05sa at c
    with dissolve

    voice "KOHARU_0162"
    koharu "「大家，夸奖得太过了啦。」"

    show amane tam02sa at l
    show koharu tko05sa at r
    with dissolve

    voice "AMANE_0458"
    amane "「没有这回事，非常好吃哦。」"

    show koharu tko02sa at r

    voice "KOHARU_0163"
    koharu "「谢谢你，天音。」"

    show kaori tka02sa at sl
    show koharu tko02sa at sr
    show amane tam02sa at c
    with dissolve

    voice "KAORI_0424"
    kaori "「这个熊猫我收下了。」"

    show amane tam03sa at c

    voice "AMANE_0459"
    amane "「啊，明明是我看中的说。让我吃一口嘛——」"

    show kaori tka01sa at sl

    voice "KAORI_0425"
    kaori "「可不让你哦——」"

    voice "KAORI_0426"
    kaori "「味道都是一样的，哪个都可以嘛。」"

    voice "AMANE_0460"
    amane "「我想全种类制霸嘛。」"

    voice "KAORI_0427"
    kaori "「不是还有熊猫剩下吗？」"

    voice "AMANE_0461"
    amane "「呜呜，有吗。」"

    voice "AMANE_0462"
    amane "「这是熊，这边是猫……」"

    show amane tam04sa at c

    voice "AMANE_0463"
    amane "「哦哦！这，这是！」"

    show kaori tka03sa at sl

    voice "KAORI_0428"
    kaori "「怎么了？」"

    show amane tam02sa at c

    voice "AMANE_0464"
    amane "「这个兔子，好像小春。」"

    show kaori tka02sa at sl

    voice "KAORI_0429"
    kaori "「啊，真的呢。」"

    show koharu tko03sa at sr

    voice "KOHARU_0164"
    koharu "「诶诶，是吗？」"

    show kaori tka01sa at sl

    voice "KAORI_0430"
    kaori "「因为小春像小动物，有仓鼠的感觉呢。」"

    show koharu tko02sa at sr

    voice "KOHARU_0165"
    koharu "「呵呵呵，仓鼠吗……」"

    voice "KOHARU_0166"
    koharu "「啊，我呢，从以前开始就想试试在那个圆圆的轮子里面跑来转去了。」"

    show kaori tka03sa at sl

    voice "KAORI_0431"
    kaori "「那个，也实在太奇怪了吧。」"

    voice "AMANE_0465"
    amane "「就是说又小又可爱哦。」"

    voice "AMANE_0466"
    amane "「拿起来，就想磨磨蹭蹭呢。」"

    show koharu tko03sa at sr

    voice "KOHARU_0167"
    koharu "「磨磨蹭蹭？」"

    hide kaori
    show amane tam02sa at sl
    show makoto tma02sa at sr
    show koharu tko03sa at c
    with dissolve

    voice "MAKOTO_0128"
    makoto "「天音的心情，我非常了解……」"

    hide koharu
    show kaori tka03sa at sl
    show amane tam02sa at c
    with dissolve

    voice "KAORI_0432"
    kaori "「你能了解么。」"

    voice "AMANE_0467"
    amane "「我是想把那又浓又短的毛，用手来回搔痒派呢。」"

    voice "MAKOTO_0129"
    makoto "「我是想抚摸抚摸派。」"

    voice "AMANE_0468"
    amane "「我是想把毛撑起来，刺刺刷刷派！」"

    show amane tam01sa at c

    voice "AMANE_0469"
    amane "「佳织酱呢？」"

    voice "KAORI_0433"
    kaori "「诶诶，我没什么……」"

    voice "AMANE_0470"
    amane "「是什么派啊？」"

    voice "KAORI_0434"
    kaori "「嗯，嗯嗯呃……」"

    hide makoto
    show koharu tko03sa at sr
    with dissolve

    voice "KOHARU_0168"
    koharu "「……」"

    voice "KAORI_0435"
    kaori "「非要说的话，想吃掉派？」"

    show koharu tko04sa at sr

    voice "KOHARU_0169"
    koharu "「咿！？」"

    hide amane
    show makoto tma04sa at sr
    show koharu tko04sa at c
    with dissolve

    voice "MAKOTO_0130"
    makoto "「！！」"

    play sound "se/SE015.ogg"

    voice "KOHARU_0170"
    koharu "「哇呀。」"

    show makoto tma08sa at sr

    voice "MAKOTO_0131"
    makoto "「我不会让任何人吃掉小春的。」"

    voice "KAORI_0436"
    kaori "「不是不是，就算你眼神这么狰狞。」"

    show makoto tma10sa at sr

    voice "MAKOTO_0132"
    makoto "「因为，说要吃掉小春……」"

    voice "KAORI_0437"
    kaori "「而且还泪流满面的。」"

    voice "MAKOTO_0133"
    makoto "「……抽泣。」"

    show kaori tka01sa at sl

    voice "KAORI_0438"
    kaori "「是可爱得，想要吃掉的意思啦。」"

    hide koharu
    show amane tam01sa at c
    with dissolve

    voice "AMANE_0471"
    amane "「把小真比作动物的话，就是鹫或者鹰呢。」"

    show makoto tma03sa at sr

    voice "MAKOTO_0134"
    makoto "「我是……鸟类？」"

    show amane tam02sa at c

    voice "AMANE_0472"
    amane "「而且，是又强又帅的大鸟。」"

    voice "MAKOTO_0135"
    makoto "「我，看起来有那么强吗。」"

    voice "AMANE_0473"
    amane "「很像很像。又是美人儿。」"

    voice "MAKOTO_0136"
    makoto "「是这样吗……」"

    show amane tam01sa at c

    voice "AMANE_0474"
    amane "「真琴，张开两手扮一下飞看看！」"

    show kaori tka03sa at sl

    voice "KAORI_0439"
    kaori "「你呀，在拜托着什么呢。」"

    show makoto tma01sa at sr

    voice "MAKOTO_0137"
    makoto "「啪萨，啪萨。」"

    show kaori tka04sa at sl

    voice "KAORI_0440"
    kaori "「呃，还真做咧！」"

    show amane tam02sa at c

    voice "AMANE_0475"
    amane "「哦哦，这正是鹫啊。」"

    hide kaori
    show amane tam02sa at sl
    show koharu tko03sa at c
    with dissolve

    voice "KOHARU_0171"
    koharu "「天音，你见过鹫吗？」"

    show amane tam01sa at sl

    voice "AMANE_0476"
    amane "「没有哦？」"

    show koharu tko04sa at c

    voice "KOHARU_0172"
    koharu "「诶诶。」"

    hide makoto
    show kaori tka03sa at sl
    show koharu tko04sa at sr
    show amane tam01sa at c
    with dissolve

    voice "KAORI_0441"
    kaori "「既然这样为什么说像鹫呢？」"

    hide kaori
    hide koharu
    show amane tam03sa at c
    with dissolve

    voice "AMANE_0477"
    amane "「是想象哦。」"

    show kaori tka03sa at l
    show amane tam03sa at r
    with dissolve

    voice "KAORI_0442"
    kaori "「顺带一问鹫和鹰的区别是？」"

    show amane tam01sa at r

    voice "AMANE_0478"
    amane "「大小不同。鹫更大吧。」"

    show kaori tka01sa at l

    voice "KAORI_0443"
    kaori "「就是说空有知识吗。」"

    voice "AMANE_0479"
    amane "「小真长得高，体型又好，所以是鹫系吧。」"

    show kaori tka01sa at sl
    show makoto tma01sa at sr
    show amane tam01sa at c
    with dissolve

    voice "MAKOTO_0138"
    makoto "「啪萨，啪萨。」"

    voice "KAORI_0444"
    kaori "「已经够了吧，那个。」"

    voice "MAKOTO_0139"
    makoto "「啪萨，啪萨。」"

    show kaori tka03sa at sl

    voice "KAORI_0445"
    kaori "「……你喜欢上了吗？」"

    voice "MAKOTO_0140"
    makoto "「还行。」"

    show kaori tka01sa at sl

    voice "KAORI_0446"
    kaori "「是么。」"

    show makoto tma08sa at sr

    voice "MAKOTO_0141"
    makoto "「可是我……喜欢大象……」"

    show kaori tka07sa at sl

    voice "KAORI_0447"
    kaori "「那算啥个葱啊——！？」"

    stop voice
    window hide

    stop music fadeout 2.0
    scene black  with fade2
    pause 1

    jump s018
